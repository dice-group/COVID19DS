<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE LIMES SYSTEM "limes.dtd">
<LIMES>
    <PREFIX>
	   	<NAMESPACE>http://xmlns.com/foaf/0.1/</NAMESPACE>
	   	<LABEL>foaf</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2002/07/owl#</NAMESPACE>
        <LABEL>owl</LABEL>
    </PREFIX>
    <PREFIX>
        <NAMESPACE>http://www.w3.org/2011/http#</NAMESPACE>
        <LABEL>http</LABEL>
    </PREFIX>
    <PREFIX>
		<NAMESPACE>http://data.semanticweb.org/ns/swc/ontology#</NAMESPACE>
		<LABEL>swc</LABEL>
    </PREFIX>
    <PREFIX>
		<NAMESPACE>http://purl.org/dc/terms/</NAMESPACE>
		<LABEL>dcterms</LABEL>
    </PREFIX>
    <PREFIX>
		<NAMESPACE>http://purl.org/net/nknouf/ns/bibtex#</NAMESPACE>
		<LABEL>bibtex</LABEL>
    </PREFIX>
    <PREFIX>
		<NAMESPACE>http://linkedgeodata.org/ontology/</NAMESPACE>
		<LABEL>lgdo</LABEL>
    </PREFIX>

	<SOURCE>
		<ID>covid</ID>
		<ENDPOINT>/home/user/Documents/work/COVID19DS/corona_new</ENDPOINT>
		<VAR>?x</VAR>
		<PAGESIZE>-1</PAGESIZE>
		<RESTRICTION>?x a bibtex:Entry</RESTRICTION>
		<PROPERTY>bibtex:hasTitle RENAME name</PROPERTY>
		<TYPE>TURTLE</TYPE>
	</SOURCE>


	<TARGET>
		<ID>covid2</ID>
		<ENDPOINT>/home/user/Documents/work/COVID19DS/corona_new</ENDPOINT>
		<VAR>?y</VAR>
		<PAGESIZE>-1</PAGESIZE>
		<RESTRICTION>?y a swc:Paper</RESTRICTION>
		<PROPERTY>dcterms:title RENAME name</PROPERTY>
		<TYPE>TURTLE</TYPE>
	</TARGET>


	<METRIC>ExactMatch(x.name,y.name)|1.0</METRIC>
	<ACCEPTANCE>
		<THRESHOLD>1.0</THRESHOLD>
		<FILE>accepted.nt</FILE>
		<RELATION>owl:sameAs</RELATION>
	</ACCEPTANCE>

	<REVIEW>
		<THRESHOLD>1.0</THRESHOLD>
		<FILE>to_review.nt</FILE>
		<RELATION>lgdo:near</RELATION>
	</REVIEW>

	<EXECUTION>
		<REWRITER>default</REWRITER>
		<PLANNER>default</PLANNER>
		<ENGINE>owl:sameAs</ENGINE>
	</EXECUTION>

	<OUTPUT>NT</OUTPUT>
</LIMES>
